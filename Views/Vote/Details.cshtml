@model IPLAwardManagementSystem.DTOs.VoteDto
@{
    ViewData["Title"] = "Vote Details";
}

<h2>Vote Details</h2>

<div>
    <p><strong>Player:</strong> @Model.PlayerName</p>
    <p><strong>Award:</strong> @Model.AwardName</p>
    <p><strong>Voter:</strong> @Model.VoterName</p>
    <p><strong>Date:</strong> @Model.VoteDate.ToString("g")</p>
</div>

<a asp-action="Index" class="btn btn-secondary">Back to List</a>
